# Frontend API base (point to Railway URL or local server)
VITE_API_BASE=http://localhost:8787
# Shared API key sent from frontend to backend
VITE_API_KEY=replace-with-shared-secret
# Password to unlock the dashboard (client-side gate)
VITE_DASH_PASSWORD=replace-with-password

# Backend RPC (not exposed to frontend)
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
# Backend shared secret (must match VITE_API_KEY)
API_KEY=replace-with-shared-secret
# Restrict backend CORS to your site (e.g., https://app.yourdomain.com)
ALLOWED_ORIGIN=http://localhost:5173
